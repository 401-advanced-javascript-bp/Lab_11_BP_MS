<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="styles/reset.css">
    <link rel="stylesheet" href="styles/base.css">
    <link rel="stylesheet" href="styles/layout.css">
    <link rel="stylesheet" href="styles/modules.css">

    <title>Document</title>
</head>

<body>
    <p><a href="/new_search">New Search</a></p>
    <h4>
        <%= savedBooksArr.length %> Saved Books</h4>
    <main>
        <% savedBooksArr.forEach(showBook => { %>
            <section>
                <img src="<%= showBook.image_url %>" alt="">
                <h2>
                    <%= showBook.title %>
                </h2>
                <p>
                    <%= showBook.author %>
                </p>
                <a href="#"><button>View Details</button></a>
            </section>
            <% }) %>
    </main>
</body>

</html>